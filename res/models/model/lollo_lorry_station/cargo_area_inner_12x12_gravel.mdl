function data()
    local mdlHelpers = require('lollo_lorry_station.mdlHelpers')
    return {
        boundingInfo = mdlHelpers.getCargoAreaInner12x12BoundingInfo(),
        collider = mdlHelpers.getCargoAreaInner12x12Collider(),
        lods = mdlHelpers.getCargoAreaInner12x12GravelLods(true),
        metadata = mdlHelpers.getVoidMetadata(),
        version = 1
    }
end
