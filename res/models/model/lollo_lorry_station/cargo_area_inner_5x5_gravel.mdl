function data()
    local mdlHelpers = require('lollo_lorry_station.mdlHelpers')
    return {
        boundingInfo = mdlHelpers.getCargoAreaInner5x5BoundingInfo(),
        collider = mdlHelpers.getCargoAreaInner5x5Collider(),
        lods = mdlHelpers.getCargoAreaInner5x5GravelLods(),
        metadata = mdlHelpers.getCargoAreaInner5x5Metadata(),
        version = 1
    }
end
