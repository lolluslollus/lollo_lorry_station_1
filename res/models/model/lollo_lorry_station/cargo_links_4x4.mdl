function data()
    local mdlHelpers = require('lollo_lorry_station.mdlHelpers')
    return {
        boundingInfo = mdlHelpers.getCargoLinksBoundingInfo(),
        collider = mdlHelpers.getCargoLinksCollider(),
        lods = mdlHelpers.getCargoLinks4x4Lods(),
        metadata = mdlHelpers.getCargoAreaInner4x4Metadata(),
        version = 1
    }
end
