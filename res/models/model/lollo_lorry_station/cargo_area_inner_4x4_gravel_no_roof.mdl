function data()
    local mdlHelpers = require('lollo_lorry_station.mdlHelpers')
    return {
        boundingInfo = mdlHelpers.getCargoAreaInner4x4BoundingInfo(),
        collider = mdlHelpers.getCargoAreaInner4x4Collider(),
        lods = mdlHelpers.getCargoAreaInner4x4GravelLods(),
        metadata = mdlHelpers.getVoidMetadata(),
        version = 1
    }
end
