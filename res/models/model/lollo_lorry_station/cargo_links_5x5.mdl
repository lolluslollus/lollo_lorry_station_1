function data()
    local mdlHelpers = require('lollo_lorry_station.mdlHelpers')
    return {
        boundingInfo = mdlHelpers.getCargoLinks5x5BoundingInfo(),
        collider = mdlHelpers.getCargoLinks5x5Collider(),
        lods = mdlHelpers.getCargoLinks5x5Lods(),
        metadata = mdlHelpers.getCargoAreaInner5x5Metadata(),
        version = 1
    }
end
