function data()
    local mdlHelpers = require('lollo_lorry_station.mdlHelpers')
    return {
        boundingInfo = mdlHelpers.getCargoAreaInner15x15BoundingInfo(),
        collider = mdlHelpers.getCargoAreaInner15x15Collider(),
        lods = mdlHelpers.getCargoAreaInner15x15GravelLods(),
        metadata = mdlHelpers.getCargoAreaInner15x15Metadata(),
        version = 1
    }
end
